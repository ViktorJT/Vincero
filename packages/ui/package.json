{
  "name": "@vincero/ui",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./banner": {
      "types": "./dist/banner/index.d.ts",
      "import": "./dist/banner/index.js"
    },
    "./carousel": {
      "types": "./dist/carousel/index.d.ts",
      "import": "./dist/carousel/index.js"
    },
    "./form": {
      "types": "./dist/contact/index.d.ts",
      "import": "./dist/contact/index.js"
    },
    "./header": {
      "types": "./dist/header/index.d.ts",
      "import": "./dist/header/index.js"
    },
    "./media": {
      "types": "./dist/media/index.d.ts",
      "import": "./dist/media/index.js"
    },
    "./overview": {
      "types": "./dist/overview/index.d.ts",
      "import": "./dist/overview/index.js"
    },
    "./slider": {
      "types": "./dist/slider/index.d.ts",
      "import": "./dist/slider/index.js"
    },
    "./text": {
      "types": "./dist/text/index.d.ts",
      "import": "./dist/text/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
