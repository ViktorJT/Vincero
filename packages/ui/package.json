{
  "name": "@vincero/ui",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "exports": {
    "./theme.css": "./src/theme.css",
    "./media": {
      "types": "./dist/media/index.d.ts",
      "import": "./dist/media/index.js"
    },
    "./header": {
      "types": "./dist/header/index.d.ts",
      "import": "./dist/header/index.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
