    "class-variance-authority": "0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "0.454.0",
    "next": "^15.0.2",
    "tailwind-merge": "^2.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@graphcms/rich-text-types": "^0.5.1",
    "@vincero/eslint-config": "workspace:*",
    "@vincero/typescript-config": "workspace:*",
    "react": "^18.2.0",
    "tsup": "^8.3.5"
