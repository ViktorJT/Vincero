{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Storybook",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declarationMap": false,
    "incremental": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "esnext"],
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "es5",
    "paths": {
      "@/*": ["./*"],
      "@vincero/ui/*": ["../../packages/ui/src/*"]
    }
  }
}
