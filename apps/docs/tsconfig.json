    "**/*.ts",
    "**/*.tsx",
    ".storybook/**/*.ts",
    ".storybook/**/*.tsx"
  ],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@vincero/ui": ["../../packages/ui"],
      "@vincero/ui/*": ["../../packages/ui/*"]
    },
  },
}
